{
    "presets": [
      "env",
      "es2015",
      "es2017"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./"],
          "alias": {
            "@lf": "./src/components/LearningFacilitator",
            "@user": "./src/components/User",
            "$routes": "./src/routes",
            "lib": "./lib",
            "_middlewares": "./src/middlewares",
            "_db": "./src/db"
          }
        }
      ],
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  }